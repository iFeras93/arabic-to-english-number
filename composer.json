{
    "name": "iferas93/arabic-to-english-number",
    "description": "Convert Arabic to English number",
    "keywords": [
        "iferas93",
        "Feras Mohamed",
        "arabic-to-english-number"
    ],
    "homepage": "https://github.com/iferas93/arabic-to-english-number",
    "license": "MIT",
    "authors": [
        {
            "name": "Feras Mohamed",
            "email": "Ferasmo7mad@gmail.com",
            "homepage": "https://iferas93.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Iferas93\\ArToEn\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iferas93\\ArToEn\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
